<% unless @external_work.blank? %>
   $('#bookmark_external_author').val("<%= @external_work.author %>").change();
   $('#bookmark_external_title').val("<%= @external_work.title %>").change();
   $('#bookmark_external_summary').val("<%= escape_javascript(@external_work.summary.html_safe) %>").change();
   $('#bookmark_external_summary').html("<%= escape_javascript(@external_work.summary.html_safe) %>").change();
   $('#fetched').val("<%= @external_work.id %>");
   $('#bookmark_external_rating_string').val("<%= @external_work.rating_string %>");
   $('#bookmark_external_category_string').val("<%= @external_work.category_string %>");
   $('#bookmark_external_fandom_string').val("<%= @external_work.fandom_string %>");
   $('#bookmark_external_relationship_string').val("<%= @external_work.relationship_string %>");
   $('#bookmark_external_character_string').val("<%= @external_work.character_string %>");
<% end %>
